{
  "name": "map-app",
  "private": true,
  "version": "1.0.0",
  "description": "Map App",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
    "format": "prettier . --write",
    "format:check": "prettier .",
    "format:report": "mkdir -p reports && prettier --list-different . > reports/prettier.txt",
    "lint": "eslint",
    "lint:sarif": "mkdir -p reports && eslint -f @microsoft/eslint-formatter-sarif -o reports/eslint.sarif",
    "lint:fix": "eslint --fix",
    "lint:watch": "onchange -i --exclude-path .gitignore './**/*.{js,mjs,cjs,jsx}' -- eslint",
    "gen:icons": "node scripts/gen-icons.mjs",
    "prepare": "husky"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "dom7": "^4.0.6",
    "framework7": "^8.3.4",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.3.4",
    "material-icons": "^1.13.14",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "skeleton-elements": "^4.0.1",
    "swiper": "^12.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@vitejs/plugin-react": "^5.0.4",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "onchange": "^7.1.0",
    "postcss-preset-env": "^10.4.0",
    "prettier": "3.6.2",
    "prop-types": "^15.8.1",
    "sharp": "^0.34.4",
    "vite": "^7.1.10",
    "workbox-cli": "^7.3.0"
  }
}
